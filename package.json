{
  "name": "gestao-politica",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc -p tsconfig.app.json --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings=0",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:run": "vitest run",
    "test:ci": "vitest run --coverage --reporter=verbose",
    "test:unit": "vitest run --reporter=verbose src/**/*.test.{ts,tsx}",
    "test:integration": "vitest run --reporter=verbose src/test/integration/**/*.test.{ts,tsx}",
    "supabase:setup": "node scripts/setup-local-supabase.js",
    "env:local": "node scripts/switch-env.cjs local",
    "env:online": "node scripts/switch-env.cjs online",
    "env:status": "node scripts/switch-env.cjs status",
    "supabase:install": "powershell -ExecutionPolicy Bypass -File scripts/install-supabase-cli.ps1",
    "check:views": "node scripts/check-views.js",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "build:analyze": "npm run build && npm run analyze:bundle",
    "performance:test": "npm run build && npm run analyze:bundle",
    "cache:clear": "node -e \"console.log('Cache cleared')\"",
    "preload:test": "node -e \"console.log('Testing preload system...')\""
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.10",
    "@googlemaps/markerclusterer": "^2.6.2",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-google-maps/api": "^2.20.7",
    "@sentry/react": "^10.19.0",
    "@sentry/vite-plugin": "^4.3.0",
    "@supabase/supabase-js": "^2.55.0",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-table": "^8.21.3",
    "@types/react-window": "^1.8.8",
    "@vercel/analytics": "^1.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.344.0",
    "pako": "^2.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "^6.28.0",
    "react-window": "^2.2.0",
    "react-window-infinite-loader": "^2.0.0",
    "recharts": "^3.1.2",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.17",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^24.3.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^27.0.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.1.9",
    "vitest": "^3.2.4"
  }
}
